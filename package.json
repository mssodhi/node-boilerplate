{
  "name": "NodeExpress",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "helmet": "^3.14.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.1",
    "sequelize": "^4.39.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "snyk": "^1.103.4"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "nodemon src/app.js",
    "local": "NODE_ENV=local nodemon src/app.js --trace-sync-io",
    "test": "snyk test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
